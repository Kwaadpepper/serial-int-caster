{
    "name": "kwaadpepper/serial-int-caster",
    "description": "Convert int To as serial number and reverse",
    "type": "library",
    "require": {
        "php": ">=7.3.0",
        "ruafozy/mersenne-twister": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "slevomat/coding-standard": "^8.6"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jérémy Munsch",
            "email": "github@jeremydev.ovh"
        }
    ],
    "autoload": {
        "psr-4": {
            "Kwaadpepper\\Serial\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "testcoverage": "vendor/bin/phpunit --test unit --coverage-html coverage",
        "generateList": "@php tests/generateTestList.php"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
